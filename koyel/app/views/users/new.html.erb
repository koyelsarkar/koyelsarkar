<h2>Create New User</h2>
<%= form_for @user do |f|%>
<div>
<%= f.label:firstname%><br/>
<%= f.text_field:firstname%>
</div>
<div>
<%= f.label:lastname%><br/>
<%= f.text_field:lastname%>
</div>
<div>
<%= f.label:email%><br/>
<%= f.text_field:email%>
</div>
<div>
<%= f.label:age%><br/>
<%= f.text_field:age%>
</div>
<div>
<%= f.label:contactno%><br/>
<%= f.text_field:contactno%>
</div>
<div>
<%= f.label:gender%><br/>
<%= f.text_field:gender%>
</div>
<div>
<%= f.label:country%><br/>
<%= f.text_field:country%>
</div>
<%= f.submit%>
<%end%>
<%= link_to "Back", users_path%>

